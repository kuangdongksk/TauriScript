{
  "identifier": "main-permissions",
  "description": "主窗口的权限",
  "windows": ["main"],
  "webview": ["main"],
  "remote": {
    "urls": ["http://localhost:1420/"]
  },
  "local": true,
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPDATA/*" }]
    },
    "notification:default",
    "sql:default",
    "store:default",
    "upload:default"
  ]
}

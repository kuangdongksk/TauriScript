{
  "identifier": "desktop-capability",
  "platforms": ["macOS", "windows", "linux"],
  "windows": ["main"],
  "webviews": ["main"],
  "remote": {
    "urls": ["http://localhost:1500/"]
  },
  "local": true,
  "permissions": [
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled",
    "window-state:default",
    "sql:default",
    "store:default",
    "fs:default",
    "opener:default",
    "upload:default"
  ]
}
